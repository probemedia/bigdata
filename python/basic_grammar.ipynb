class Car():
    def __init__(self, displ, drv):
        self.__displ = displ
        self.__drv = drv

    def move(self, speed):
        print("자동차 이동 속도 : 시속 {0}km".format(speed))

    def info(self):
        print("자동차 정보 : ({0}, {1}구동)".format(self.__displ, self.__drv))


car1 = Car(3000, '4륜')
car1.info()
